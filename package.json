{
  "name": "nextjs_crash_course",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "server:dev": "next build && sls offline start",
    "server:prod": "NODE_ENV=production sls offline start",
    "build": "next build",
    "build:test:production": "next build && serverless package",
    "export": "rm -rf ../temp-next-static-site && next build && next export -o ../temp-next-static-site && npm run delete:except:git && mv ../temp-next-static-site/* ../next-static-site",
    "export:help": "next export -h",
    "export:test": "next build && next export",
    "export:test:ex": "next export -o ../temp-next-static-site",
    "delete:except:git": "find ../next-static-site -not -path '*.git*' -not -path '*next-static-site' -print0 | xargs -0 rm -rf",
    "start": "next build && next start",
    "deploy": "-",
    "deploy:aws": "next build && aws s3 cp ../next-static-site s3://nextjs-geoseong --recursive --acl public-read --profile 404hunter-geoseong",
    "deploy:serverless": "next build && sls deploy -v --aws-profile 404hunter-geoseong",
    "deploy:test:production": "npm run build:production && serverless deploy -v --package .serverless",
    "deploy:gitpage": "next build && next export && cp sitemap.xml out/ && gh-pages -d out -b master",
    "gitpage": "gh-pages -d out -b master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "gh-pages": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "path": "^0.12.7",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "serverless-http": "^1.8.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.6.0",
    "next-offline": "^3.2.2",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-webpack": "^5.2.0",
    "webpack-node-externals": "^1.7.2"
  }
}
